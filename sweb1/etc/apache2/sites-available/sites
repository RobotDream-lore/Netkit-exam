<VirtualHost *:80>
ServerAdmin bt7274@localhost
ServerName folder.titan.fall
DocumentRoot /var/www/html/folder
ServerPath "/"
Options -Indexes
<Directory "/">
Options FollowSymLinks MultiViews Indexes
AllowOverride all
Order allow,deny
Allow from all
</Directory>
</VirtualHost>

